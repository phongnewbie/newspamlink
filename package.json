{
  "name": "spamlink",
  "version": "1.0.0",
  "description": "Spam Link Management System",
  "scripts": {
    "install-server": "cd server && npm install",
    "install-frontend": "cd frontend/customerweb && npm install",
    "build-frontend": "cd frontend/customerweb && npm run build",
    "build": "npm run install-server && npm run install-frontend && npm run build-frontend",
    "start": "cd server && npm start"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "geoip-lite": "^1.4.10",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "xlsx": "^0.18.5"
  }
}
